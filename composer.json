{
    "name": "compwright/psr-cors",
    "description": "PHP interop cross-origin resource sharing (CORS) library and middleware",
    "keywords": ["psr-7", "psr-15", "cors"],
    "homepage": "https://github.com/compwright/psr-cors",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathon Hill",
            "email": "jonathon@compwright.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "psr/http-message": "^1.0 || ^2.0",
        "psr/http-factory": "^1.1",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "nyholm/psr7": "^1.8",
        "phpstan/phpstan": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.65"
    },
    "autoload": {
        "psr-4": {
            "Compwright\\PsrCors\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Compwright\\PsrCors\\": "tests/"
        }
    }
}
